<h1> Discover Movies </h1>
<%= button_to "Discover Top 40 Movies", discover_movies_path, method: :get, :params => {:top40 => true}%>
<h3>Search For Movies </h3>
<%= form_with url: "/discover/movies", method: :get, local: true do |f| %>
  <%= f.text_field :search %>
  <%= f.submit "Search" %>
<% end %>
