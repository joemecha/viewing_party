<h1> Discover Movies </h1>
<%= button_to "Discover Top 40 Movies"%>

<h3>Search For Movies </h3>
<%= form_with url: "/discover", method: :get, local: true do |f| %>
<%= f.text_field :search %>
<%= f.submit "Search" %>
<% end %>

<% if defined?(movie_search) %>
<h3>Search Results</h3>
<% movie_search.each do |movie| %>
<%= link_to "#{movie.title}", "/discover/movies/WHAT TO PUT HERE?" %>
<%= movie.vote_average %>
<% end %>
<% end %>